<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>

	<!--
		###############
		### 1st tab: General
		###############
	-->
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:rggooglemap/locallang_flex.xml:general.sheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<show>
						<TCEforms>
							<label>LLL:EXT:rggooglemap/locallang_flex.xml:general.show</label>
							<config>
								<type>select</type>
								<maxitems>10</maxitems>
								<minitems>1</minitems>
								<itemsProcFunc>tx_rggooglemap_itemsProcFunc->user_insertExtraCodes</itemsProcFunc>
								<size>6</size>
								<selectedListStyle>width:150px</selectedListStyle>
								<itemListStyle>width:150px</itemListStyle>
								
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:rggooglemap/locallang_flex.xml:general.show.0</numIndex>
										<numIndex index="1">MAP</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:rggooglemap/locallang_flex.xml:general.show.6</numIndex>
										<numIndex index="1">CATMENU</numIndex>
									</numIndex>	
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:rggooglemap/locallang_flex.xml:general.show.2</numIndex>
										<numIndex index="1">MENU</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:rggooglemap/locallang_flex.xml:general.show.3</numIndex>
										<numIndex index="1">LOCATION</numIndex>
									</numIndex>								
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:rggooglemap/locallang_flex.xml:general.show.4</numIndex>
										<numIndex index="1">RECORDSONMAP</numIndex>
									</numIndex>	
									<numIndex index="5" type="array">
										<numIndex index="0">LLL:EXT:rggooglemap/locallang_flex.xml:general.show.5</numIndex>
										<numIndex index="1">SEARCH</numIndex>
									</numIndex>	
									<numIndex index="7" type="array">
										<numIndex index="0">LLL:EXT:rggooglemap/locallang_flex.xml:general.show.7</numIndex>
										<numIndex index="1">DIRECTIONS</numIndex>
									</numIndex>	
								</items>
							</config>
						</TCEforms>
					</show>
				
					<key>
						<TCEforms>
							<label>LLL:EXT:rggooglemap/locallang_flex.xml:general.key</label>
							<config>
								<type>input</type>
								<size>34</size>
							</config>
						</TCEforms>
					</key>

					<categoriesselected>
						<TCEforms>
							<label>LLL:EXT:rggooglemap/locallang_flex.xml:general.categoriesselected</label>
							<config>
								<type>select</type>
								<foreign_table>tx_rggooglemap_cat</foreign_table>
								<foreign_table_where>ORDER BY tx_rggooglemap_cat.title ASC</foreign_table_where>
								<size>6</size>
								<maxitems>42</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</categoriesselected>	
					
					<categories>
						<TCEforms>
							<label>LLL:EXT:rggooglemap/locallang_flex.xml:general.categories</label>
							<config>
								<type>select</type>
								<foreign_table>tx_rggooglemap_cat</foreign_table>
								<foreign_table_where>ORDER BY tx_rggooglemap_cat.title ASC</foreign_table_where>
								<size>6</size>
								<maxitems>42</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</categories>

					<startingpoint>
						<TCEforms>
							<label>LLL:EXT:lang/locallang_general.xml:LGL.startingpoint</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>3</size>
								<maxitems>22</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</startingpoint>

					<recursive>
						<TCEforms>
							<label>LLL:EXT:lang/locallang_general.xml:LGL.recursive</label>
							<config>
								<type>select</type>
									<items type="array">
										<numIndex index="0" type="array">
											<numIndex index="0"></numIndex>
											<numIndex index="1"></numIndex>
										</numIndex>
										<numIndex index="1" type="array">
											<numIndex index="0">LLL:EXT:cms/locallang_ttc.xml:recursive.I.1</numIndex>
											<numIndex index="1">1</numIndex>
										</numIndex>
										<numIndex index="2" type="array">
											<numIndex index="0">LLL:EXT:cms/locallang_ttc.xml:recursive.I.2</numIndex>
											<numIndex index="1">2</numIndex>
										</numIndex>
										<numIndex index="3" type="array">
											<numIndex index="0">LLL:EXT:cms/locallang_ttc.xml:recursive.I.3</numIndex>
											<numIndex index="1">3</numIndex>
										</numIndex>
										<numIndex index="4" type="array">
											<numIndex index="0">LLL:EXT:cms/locallang_ttc.xml:recursive.I.4</numIndex>
											<numIndex index="1">4</numIndex>
										</numIndex>
										<numIndex index="5" type="array">
											<numIndex index="0">LLL:EXT:cms/locallang_ttc.xml:recursive.I.5</numIndex>
											<numIndex index="1">250</numIndex>
										</numIndex>
									</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</recursive>

				</el>
			</ROOT>
		</sDEF>

	<!--
		###############
		### 2nd tab: Map
		###############
	-->
